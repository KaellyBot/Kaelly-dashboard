<html>
    <%- include('partials/head') %>
    <body>
        <%- include('partials/nav', {user: locals.user ? user : null }) %>
        <div class="container">
            <div class="row">
                <h1 class="header"><%= guild.name %></h1>

                <!-- Almanax -->
                <div class="col s12 l6">
                    <div class="card-panel grey lighten-5 z-depth-1">
                        <div class="valign-wrapper">
                            <div class="col s3 m2">
                                <img src="/img/almanax.png" class="circle responsive-img">
                            </div>
                            <div class="col s9 m10 guildOverview valign-wrapper">
                                    <div class="col s12 l6 guildName">
                                        Almanax
                                    </div>
                                    <div class="col s12 l6">
                                        <a href="/guild/<%= guild.id %>/almanax" class="waves-effect waves-light btn-small">
                                            <i class="material-icons">keyboard_arrow_right</i>
                                        </a>
                                    </div>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Dofus Server -->
                <div class="col s12 l6">
                    <div class="card-panel grey lighten-5 z-depth-1">
                        <div class="valign-wrapper">
                            <div class="col s3 m2">
                                <img src="/img/server.png" class="circle responsive-img">
                            </div>
                            <div class="col s9 m10 guildOverview valign-wrapper">
                                    <div class="col s12 l6 guildName">
                                        Dofus Server
                                    </div>
                                    <div class="col s12 l6">
                                        <a href="/guild/<%= guild.id %>/server" class="waves-effect waves-light btn-small">
                                            <i class="material-icons">keyboard_arrow_right</i>
                                        </a>
                                    </div>
                            </div>
                        </div>
                    </div>
                </div>

                 <!-- Language -->
                 <div class="col s12 l6">
                    <div class="card-panel grey lighten-5 z-depth-1">
                        <div class="valign-wrapper">
                            <div class="col s3 m2">
                                <img src="/img/language.png" class="circle responsive-img">
                            </div>
                            <div class="col s9 m10 guildOverview valign-wrapper">
                                    <div class="col s12 l6 guildName">
                                        Language
                                    </div>
                                    <div class="col s12 l6">
                                        <a href="/guild/<%= guild.id %>/language" class="waves-effect waves-light btn-small">
                                            <i class="material-icons">keyboard_arrow_right</i>
                                        </a>
                                    </div>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Twitter -->
                <div class="col s12 l6">
                    <div class="card-panel grey lighten-5 z-depth-1">
                        <div class="valign-wrapper">
                            <div class="col s3 m2">
                                <img src="/img/twitter.png" class="circle responsive-img">
                            </div>
                            <div class="col s9 m10 guildOverview valign-wrapper">
                                    <div class="col s12 l6 guildName">
                                        Twitter
                                    </div>
                                    <div class="col s12 l6">
                                        <a href="/guild/<%= guild.id %>/twitter" class="waves-effect waves-light btn-small">
                                            <i class="material-icons">keyboard_arrow_right</i>
                                        </a>
                                    </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- RSS -->
                <div class="col s12 l6">
                    <div class="card-panel grey lighten-5 z-depth-1">
                        <div class="valign-wrapper">
                            <div class="col s3 m2">
                                <img src="/img/rss.png" class="circle responsive-img">
                            </div>
                            <div class="col s9 m10 guildOverview valign-wrapper">
                                    <div class="col s12 l6 guildName">
                                        RSS
                                    </div>
                                    <div class="col s12 l6">
                                        <a href="/guild/<%= guild.id %>/rss" class="waves-effect waves-light btn-small">
                                            <i class="material-icons">keyboard_arrow_right</i>
                                        </a>
                                    </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </body>
    <%- include('partials/scripts') %>
</html>