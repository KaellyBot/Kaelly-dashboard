<div class="col s12 l6">
    <div class="card-panel grey lighten-5 z-depth-1">
        <div class="valign-wrapper">
            <div class="col s3 m2">
                <img src="<%= setting.icon %>" class="circle responsive-img">
            </div>
            <div class="col s9 m10 panelOverview valign-wrapper grey-text text-darken-3">
                <div class="col s12 l6 panelName">
                    <%= setting.name %>
                </div>
                <div class="col s12 l6">
                    <a href="<%= setting.url %>" class="waves-effect waves-light btn-small grey darken-2">
                        Manage
                        <i class="material-icons right">keyboard_arrow_right</i>
                    </a>
                </div>
            </div>
        </div>
    </div>
</div>